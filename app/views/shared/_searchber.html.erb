<div class="SearchRoomsFormContainer d-flex justify-content-center">
<%= form_with(url: search_rooms_path, local: true, class:"SearchRoomsForm d-flex gap-3 my-auto",method: :get) do |f| %>
  <%= f.text_field :area, placeholder: "エリアを入力", class: "form-control"%>
  <%= f.text_field :keyword, placeholder: "キーワードを入力", class: "form-control" %>
  <%= f.submit "検索", class: "btn btn-primary w-25" %>
<% end %>
</div>